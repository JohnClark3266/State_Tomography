# 1. 定义目标函数
def my_function(x, y):
    return np.sin(x) * np.cos(y)

# 2. 创建积分器
integrator = ActiveLearningIntegrator(
    target_function=my_function,
    domain=[(0, np.pi), (0, np.pi)],  # 积分区域
    Ln=1000,  # 总点数
    S0=50,    # 初始点数
    St=20,    # 每轮添加点数
    m=10      # 最大轮次
)

# 3. 运行主动学习
integrator.active_learning_cycle()

# 4. 获取结果
integral = integrator.estimate_integral()